<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- Start of Constructor Injection Logic-->
    <bean id="beanFactory" class="com.bhavesh16281.beanFactoryIOC.BeanFactoryIOC">
    	<property name="laptop" ref="laptop"></property>
    </bean>
    
    <bean id="laptop" class="com.bhavesh16281.beanFactoryIOC.Laptop"></bean>
    <!-- End of Constructor Injection Logic-->
    
</beans>