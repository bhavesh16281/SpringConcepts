<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    <!-- Start of Setter Injection Logic -->
    <bean id="setterInjection" class="com.bhavesh16281.setterInjection.SetterInjection">
    	<property name="bike" ref="bike"></property>
    </bean>
    
    <bean id="bike" class="com.bhavesh16281.setterInjection.Bike"></bean>
    <!-- End of Setter Injection Logic -->
    
</beans>